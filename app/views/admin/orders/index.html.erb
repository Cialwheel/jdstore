<h2>Order List</h2>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>#</th>
      <th>Create_time</th>
      <th>Order_user</th>
      <th>Order_state</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
    <tr>
      <td><%= link_to(order.id, admin_order_path(order)) %></td>
      <td><%= order.created_at %></td>
      <td><%= order.user.email %></td>
      <td><%= order.aasm_state %></td>
    </tr>
    <% end %>

  </tbody>
</table>
